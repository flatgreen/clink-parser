-- youtube-dl_parser.lua
--------------------------------------------------------------------------------
-- This file is a Lua script helper for youtube-dl :
--     https://github.com/rg3/youtube-dl
-- with clink : http://mridgers.github.io/clink/

ytdl_parser = clink.arg.new_parser()
ytdl_parser:set_flags({{flags}})

clink.arg.register_parser("youtube-dl", ytdl_parser)
